[
  {
    "name": "zx/core",
    "path": ["build/core.cjs", "build/util.cjs", "build/vendor-core.cjs"],
    "limit": "70 kB",
    "brotli": false,
    "gzip": false
  },
  {
    "name": "zx/index",
    "path": "build/*.{js,cjs}",
    "limit": "795 kB",
    "brotli": false,
    "gzip": false
  },
  {
    "name": "dts libdefs",
    "path": "build/*.d.ts",
    "limit": "35 kB",
    "brotli": false,
    "gzip": false
  },
  {
    "name": "vendor",
    "path": "build/vendor-*",
    "limit": "760 kB",
    "brotli": false,
    "gzip": false
  },
  {
    "name": "all",
    "path": "build/*",
    "limit": "830 kB",
    "brotli": false,
    "gzip": false
  }
]
